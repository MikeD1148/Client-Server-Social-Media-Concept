<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="385.0" prefWidth="571.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChatRoom2">
    <children>
        <Rectangle fx:id="background" arcHeight="5.0" arcWidth="5.0" fill="#e2729d" height="400.0" layoutX="-1.0" layoutY="-1.0" stroke="BLACK" strokeType="INSIDE" width="600.0" />
        <TextArea fx:id="textArea" layoutX="190.0" layoutY="162.0" prefHeight="158.0" prefWidth="209.0" />
        <TextField fx:id="Chat" layoutX="190.0" layoutY="117.0" prefHeight="43.0" prefWidth="152.0" promptText="Type Message">
            <font>
                <Font name="Cambria" size="12.0" />
            </font></TextField>
        <Button fx:id="send" layoutX="348.0" layoutY="117.0" mnemonicParsing="false" onAction="#sendOnAction" prefHeight="43.0" prefWidth="51.0" text="Send" textFill="#67c3da">
            <font>
                <Font name="Corbel" size="14.0" />
            </font></Button>
        <Button fx:id="delete" layoutX="190.0" layoutY="326.0" mnemonicParsing="false" onAction="#deleteOnAction" prefHeight="43.0" prefWidth="99.0" text="Delete" textFill="#67c3da">
            <font>
                <Font name="Corbel" size="18.0" />
            </font></Button>
        <Label layoutX="7.0" layoutY="-1.0" prefHeight="127.0" prefWidth="222.0" text="SHEFFIELD HALLAM UNIVERSITY" textFill="WHITE" wrapText="true">
            <font>
                <Font name="Elephant" size="27.0" />
            </font>
        </Label>
        <Button fx:id="refresh" layoutX="299.0" layoutY="325.0" mnemonicParsing="false" onAction="#refreshOnAction" prefHeight="43.0" prefWidth="99.0" text="Refresh" textFill="#67c3da">
            <font>
                <Font name="Corbel" size="18.0" />
            </font></Button>
        <Button fx:id="returns" layoutX="499.0" layoutY="14.0" mnemonicParsing="false" onAction="#returnOnAction" text="Back" textFill="#67c3da">
            <font>
                <Font name="Cambria" size="12.0" />
            </font></Button>
    </children>
</AnchorPane>
